{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "default": "New Property Title"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "price": {
      "type": "biginteger",
      "default": "200000"
    },
    "description": {
      "type": "richtext",
      "default": "This is the property description"
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "components.feature"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
